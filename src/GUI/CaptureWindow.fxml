<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="612.0" prefWidth="777.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CaptureWindowController">
   <children>
      <TableView fx:id="PacketsTable" layoutX="12.0" layoutY="60.0" onMouseClicked="#ShowHexValues" onSort="#StartBtn" prefHeight="200.0" prefWidth="753.0">
        <columns>
          <TableColumn fx:id="No" prefWidth="75.0" text="No." />
          <TableColumn fx:id="Time" prefWidth="75.0" text="Time" />
            <TableColumn fx:id="Source" prefWidth="75.0" text="Source" />
            <TableColumn fx:id="Destination" prefWidth="75.0" text="Destination" />
            <TableColumn fx:id="Protocol" prefWidth="75.0" text="Protocol" />
            <TableColumn fx:id="Length" prefWidth="75.0" text="Length" />
            <TableColumn fx:id="Info" prefWidth="75.0" text="Info" />
        </columns>
      </TableView>
      <Button fx:id="StartBtn" layoutX="14.0" layoutY="26.0" mnemonicParsing="false" onAction="#StartBtn" text="Start" />
      <Button fx:id="StopBtn" layoutX="64.0" layoutY="26.0" mnemonicParsing="false" onAction="#EndBtn" text="Stop" />
      <Button layoutX="115.0" layoutY="26.0" mnemonicParsing="false" text="reset" />
      <TextArea fx:id="InfoText" layoutX="12.0" layoutY="260.0" prefHeight="200.0" prefWidth="753.0" />
      <TextArea fx:id="HEXText" editable="false" layoutX="10.0" layoutY="460.0" prefHeight="200.0" prefWidth="753.0" />
      <Button fx:id="LoadBtn" layoutX="599.0" layoutY="13.0" mnemonicParsing="false" onAction="#LoadBtbClicked" text="Load" />
      <Button layoutX="667.0" layoutY="13.0" mnemonicParsing="false" onAction="#bonusTest" text="Bonus test" />
   </children>
</AnchorPane>
